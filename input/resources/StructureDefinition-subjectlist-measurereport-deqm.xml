      <condition value="deqm-1"/>
      <constraint>
        <key value="deqm-1"/>
        <severity value="error"/>
        <human value="Date precision SHALL be at least to day ( YYYY-MM-DD )"/>
        <expression value="start.toString().length()>9 and end.toString().length()>9"/>
